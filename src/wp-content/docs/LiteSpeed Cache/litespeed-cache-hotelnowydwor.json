        ".ct-",
        ".oxy-",
        "#ct-",
        "[id^=\"ct-\"]",
        "[class*=\"oxygen-\"]"
      ],
      "protected_inline_scripts": [
        "oxygenVSBFrontendData",
        "ct_",
        "ctc"
      ]
    },
    
    "hotel_specific": {
      "_comment": "Hotel Nowy Dwór specific settings",
      "booking_widget_exclusions": [
        "/rezerwacja/",
        "/booking/",
        "nfhotel-booking",
        "booking-engine"
      ],
      "high_priority_pages": [
        "/",
        "/pokoje/",
        "/restauracja/",
        "/kontakt/"
