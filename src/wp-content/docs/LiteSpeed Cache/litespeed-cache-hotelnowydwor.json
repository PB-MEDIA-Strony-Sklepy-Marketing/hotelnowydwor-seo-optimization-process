    "debug": {
      "debug_enabled": false,
      "debug_ips": "",
      "debug_level": 2,
      "debug_file_count": 5,
      "debug_file_size": 2,
      "debug_log_filters": false,
      "debug_log_no_filters": false,
      "debug_log_part_filters": false,
      "debug_disable_all": false,
      "debug_log_purge": true,
      "debug_log_query_string": false,
      "debug_log_cookie": false,
      "debug_log_header": false
