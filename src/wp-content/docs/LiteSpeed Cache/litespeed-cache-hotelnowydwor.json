{
  "litespeed.conf.version": "7.7",
  "data": {
    "_comment_header": {
      "version": "7.7",
      "created": "2025-12-28",
      "site": "https://www.hotelnowydwor.eu/",
      "description": "LiteSpeed Cache configuration for Hotel Nowy Dwór - WordPress + Oxygen Builder",
      "target": "PageSpeed Score >= 90"
    },
    
    "general": {
      "enabled": true,
      "cache_status": "on",
      "auto_upgrade": false,
      "api_key": "",
      "server_ip": "",
      "notifications": false,
      "heartbeat_front": true,
      "heartbeat_front_ttl": 60,
      "heartbeat_back": true,
      "heartbeat_back_ttl": 60,
      "heartbeat_editor": false,
      "heartbeat_editor_ttl": 60
    },
    
    "cache": {
      "cache_priv": true,
      "cache_commenter": true,
      "cache_rest": true,
      "cache_page_login": false,
      "cache_favicon": true,
      "cache_resources": true,
      "cache_mobile": true,
      "mobile_rules": [
        "Mobile",
        "Android",
        "Silk/",
        "Kindle",
        "BlackBerry",
        "Opera Mini",
        "Opera Mobi",
        "webOS"
      ],
      "cache_browser": true,
      "login_cookie": "",
      "improve_http2": false
    },
    
    "ttl": {
      "pub_ttl": 604800,
      "priv_ttl": 1800,
      "front_page_ttl": 604800,
      "feed_ttl": 604800,
      "rest_ttl": 604800,
      "status_404_ttl": 3600,
      "status_403_ttl": 3600,
      "status_500_ttl": 3600
    },
    
    "purge": {
      "auto_purge_time": "",
      "timed_urls": "",
      "timed_urls_time": "",
      "purge_post_update": true,
      "purge_on_post_update": [
        "frontpage",
        "homepage",
        "pages",
        "categories",
        "tags",
        "archives",
        "author",
        "terms",
        "sitemap",
        "amp"
      ],
      "purge_stale": true,
      "stale_ttl": 86400
    },
    
    "excludes": {
      "excludes_uri": [
        "/wp-admin/",
        "/wp-login.php",
        "/koszyk/",
        "/checkout/",
        "/moje-konto/",
        "/rezerwacja/",
        "/oxygenim/",
        "/?ct_builder=",
        "/?oxygen_iframe=",
        "/ct_template/",
        "/wp-json/wp/v2/users",
        "/wp-json/oembed/"
      ],
      "excludes_qs": [
        "fbclid",
        "gclid",
        "utm_*",
        "mc_*",
        "preview",
        "customize_changeset_uuid",
        "ct_builder",
        "oxygen_iframe",
        "vc_editable",
        "elementor-preview"
      ],
      "excludes_cat": [],
      "excludes_tag": [],
      "excludes_cookie": [
        "wordpress_logged_in_",
        "comment_author_",
        "woocommerce_",
        "wp_woocommerce_session_"
      ],
      "excludes_ua": [
        "PayPal",
        "googlebot-image",
        "Stripe"
      ],
      "excludes_roles": [
        "administrator",
        "editor"
      ]
    },
    
    "esi": {
      "esi_enabled": false,
      "esi_cache_admbar": false,
      "esi_cache_commform": false,
      "esi_nonces": []
    },
    
    "drop_qs": {
      "drop_qs": [
        "fbclid",
        "gclid",
        "utm_source",
        "utm_medium",
        "utm_campaign",
        "utm_term",
        "utm_content",
        "mc_cid",
        "mc_eid",
        "_ga",
        "_gid"
      ]
    },
    
    "optm": {
      "optm_qs": true,
      "optm_uri_exc": [
        "/wp-admin/",
        "/?ct_builder=",
        "/?oxygen_iframe=",
        "/feed/"
      ],
      "optm_uri_exc_roles": [
        "administrator"
      ]
    },
    
    "css": {
      "css_minify": true,
      "css_combine": true,
      "css_inline_min": 1024,
      "css_async": true,
      "css_async_inline": true,
      "css_font_display": "swap",
      "optm_ucss": true,
      "optm_ucss_async": true,
      "optm_ucss_inline": true,
      "optm_ucss_whitelist": [
        ".ct-",
        ".oxy-",
        ".oxygen-",
        "#ct-",
        ".slick-",
        ".swiper-",
        "[class*=\"ct-\"]",
        "[class*=\"oxy-\"]"
      ],
      "optm_ccss": true,
      "optm_ccss_per_url": false,
      "optm_ccss_separate_post": false,
      "css_exc": [
        "oxygen",
        "wp-admin",
        "ct-",
        "nfhotel"
      ]
    },
    
    "js": {
      "js_minify": true,
      "js_combine": true,
      "js_inline_min": 1024,
      "js_defer": true,
      "js_delay": true,
      "js_delay_timeout": 2000,
      "js_delay_items": [
        "gtag",
        "gtm.js",
        "analytics.js",
        "facebook",
        "fbevents.js",
        "hotjar",
        "clarity.js",
        "recaptcha",
        "disqus",
        "twitter",
        "linkedin",
        "pinterest",
        "instagram"
      ],
      "optm_js_exc": [
        "jquery.js",
        "jquery.min.js",
        "jquery-migrate.min.js",
        "oxygen",
        "ct-",
        "nfhotel-booking",
        "booking-engine",
        "wp-includes/js/dist/"
      ],
      "js_defer_exc": [
        "jquery.js",
        "jquery.min.js",
        "oxygen",
        "ct-",
        "nfhotel"
      ],
      "js_inline_defer_exc": [
        "CDATA",
        "ctc-",
        "oxygen",
        "ct_"
      ]
    },
    
    "html": {
      "html_minify": true,
      "optm_emoji": true,
      "optm_noscript_rm": true,
      "optm_ggfonts_async": true,
      "optm_ggfonts_rm": false,
      "optm_dns_prefetch": [
        "//fonts.googleapis.com",
        "//fonts.gstatic.com",
        "//www.google-analytics.com",
        "//www.googletagmanager.com",
        "//connect.facebook.net",
        "//www.google.com",
        "//maps.googleapis.com",
        "//maps.google.com"
      ],
      "optm_dns_prefetch_ctrl": true,
      "optm_exc_jquery": true,
      "optm_inline_style": true,
      "optm_exc_roles": [
        "administrator"
      ]
    },
    
    "media": {
      "media_lazyload": true,
      "media_lazyload_basic_img": true,
      "media_lazyload_bg": true,
      "media_lazyload_video": true,
      "media_lazyload_inline": true,
