  "meta": {
    "exported_at": "2025-12-28T12:00:00Z",
    "litespeed_version": "7.7",
    "wordpress_version": "6.4+",
    "php_version": "8.0+",
    "server_type": "LiteSpeed/OpenLiteSpeed",
    "notes": [
      "Configuration optimized for Hotel Nowy Dwór website",
      "WordPress with Oxygen Builder page builder",
      "Target: PageSpeed Score >= 90 (Mobile & Desktop)",
      "Includes compatibility settings for NFHotel booking widget",
      "QUIC.cloud integration ready (requires API key)",
      "Object Cache disabled by default (enable if Redis/Memcached available)",
      "Crawler configured for automatic cache warming",
      "Critical CSS generation via QUIC.cloud",
      "WebP conversion enabled for images",
      "Lazy loading with LCP image exclusions"
    ],
    "post_import_checklist": [
      "1. Update Server IP in Crawler > General Settings",
      "2. Add QUIC.cloud API key if using their CDN",
      "3. Enable Object Cache if Redis/Memcached is available",
      "4. Verify Oxygen Builder elements render correctly",
      "5. Test NFHotel booking widget functionality",
      "6. Run PageSpeed test to verify improvement",
      "7. Check lazy loading excludes hero images",
      "8. Verify Critical CSS generation",
      "9. Test mobile responsiveness",
      "10. Monitor error logs for first 24 hours"
    ]
  }
