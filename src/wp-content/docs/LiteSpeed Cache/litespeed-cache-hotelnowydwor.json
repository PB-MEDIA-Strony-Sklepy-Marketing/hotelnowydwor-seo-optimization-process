        "/wp-admin/",
        "/?ct_builder=",
        "/?oxygen_iframe="
      ],
      "media_lazyload_cls_exc": [
        "skip-lazy",
        "no-lazy",
        "no-lazyload",
        "essential-image",
        "ct-image",
        "hero-bg",
        "lcp-image",
        "above-fold"
      ],
      "media_lazyload_parent_cls_exc": [
        "slick-",
        "swiper-",
        "hero-section",
        "ct-section-inner-wrap"
      ],
      "media_lqip": true,
      "media_lqip_quality": 4,
      "media_lqip_min": 200,
      "media_placeholder_resp": true,
      "media_placeholder_resp_color": "#f7f7f7",
      "media_placeholder_resp_css": "",
      "media_placeholder_resp_async": true,
      "media_vpi": true,
      "media_vpi_cron": true,
      "media_iframe_lazy": true,
      "media_add_missing_sizes": true,
      "media_resp_placeholder": true,
      "media_resp_placeholder_color": "#f7f7f7",
      "media_resp_placeholder_svg": true,
      "media_resp_placeholder_svg_color": "#f7f7f7"
